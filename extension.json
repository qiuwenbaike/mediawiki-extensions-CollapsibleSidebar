{
	"name": "CollapsibleSidebar",
	"version": "0.4.0",
	"author": [ "MusikAnimal", "R4356th", "AnYi", "WaitSpring" ],
	"url": "https://git.qiuwen.wiki/Fork/mediawiki-extensions-CollapsibleSidebar",
	"descriptionmsg": "collapsiblesidebar-desc",
	"license-name": "GPL-3.0",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\CollapsibleSidebar\\": "includes/"
	},
	"Hooks": {
		"BeforePageDisplay": "main"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\CollapsibleSidebar\\Hooks",
			"services": [ "MainConfig" ]
		}
	},
	"MessagesDirs": {
		"CollapsibleSidebar": [ "i18n" ]
	},
	"ResourceModules": {
		"ext.CollapsibleSidebar.js": {
			"es6": true,
			"packageFiles": [ "ext.CollapsibleSidebar.js" ],
			"messages": [
				"collapsiblesidebar-link",
				"collapsiblesidebar-link-tooltip",
				"collapsiblesidebar-default-link",
				"collapsiblesidebar-default-link-tooltip"
			]
		},
		"ext.CollapsibleSidebar.css": {
			"styles": [ "ext.CollapsibleSidebar.less" ]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "CollapsibleSidebar/resources"
	},
	"manifest_version": 2
}
